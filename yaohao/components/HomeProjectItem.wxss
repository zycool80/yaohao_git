.init__project {
    width: 210rpx!important;
}

.init__project .imageContent {
    background: #f2f2f2;
}

.init__project .name {
    width: 100%;
    background-color: #f2f2f2;
}

.init__project .label {
    width: 90rpx;
    background-color: #f2f2f2;
}

.gray-shadow {
    position: absolute;
    width: 100%;
    height: 120rpx;
    bottom: 0;
    left: 0;
    z-index: 2;
    background: linear-gradient(to top,rgba(0,0,0,0.7) 0%,transparent 30%);
}