<view class="page">
    <view class="reportType-title fz-12 small-level-color">服务声明</view>
    <view class="reportType">
        <navigator class="reportType-line" hoverClass="none" url="{{version}}">
            <text class="fz-13 black-1a-color left">版权声明</text>
            <text class="iconfont icon-xiayibu right gray-color"></text>
        </navigator>
        <navigator class="reportType-line" hoverClass="none" url="{{disclaimer}}">
            <text class="fz-13 black-1a-color left">免责声明</text>
            <text class="iconfont icon-xiayibu right gray-color"></text>
        </navigator>
        <navigator class="reportType-line" hoverClass="none" url="{{privacy}}">
            <text class="fz-13 black-1a-color left">隐私保护</text>
            <text class="iconfont icon-xiayibu right gray-color"></text>
        </navigator>
    </view>
</view>
<view class="back-home" style="bottom:{{$BackHome$bottom}}rpx" wx:if="{{showHome}}">
    <navigator openType="{{$BackHome$openType}}" url="{{$BackHome$url}}">
        <text class="backHome fz-12 white-color">{{$BackHome$text}}</text>
    </navigator>
</view>
