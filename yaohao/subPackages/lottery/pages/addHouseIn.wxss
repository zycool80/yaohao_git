@import "../__wuBaseWxss__/0.wxss";

page {
    background-color: #f7f7f7;
}

.house_plan_title {
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 20rpx 30rpx;
}

.house_plan_text {
    font-size: 24rpx;
    color: #aeaeae;
}

.hou_plan_from {
    background-color: white;
    padding-left: 30rpx;
    font-size: 26rpx;
}

.general_style {
    display: flex;
    align-items: center;
    position: relative;
}

.general_style .red-tip {
    position: absolute;
    right: -14rpx;
    top: -10rpx;
}

.text-weight {
    font-weight: 700;
}

.hou_plan_item {
    height: 90rpx;
}

.hou_plan_live {
    border-bottom: 1px solid #e6e6e6;
    width: 522rpx;
    height: 100%;
    justify-content: space-between;
    padding-right: 30rpx;
    margin-top: 10rpx;
}

.hou_plan_live2 {
    border-bottom: 1px solid #e6e6e6;
    width: 552rpx;
    height: 100%;
    margin-top: 10rpx;
}

.general_style2 {
    display: flex;
}

.house_item {
    width: 70rpx;
    margin-top: 10rpx;
}

.general_style_font {
    padding-left: 14rpx;
}

.xian {
    height: 100%;
    display: flex;
    align-items: center;
    width: 552rpx;
    border-bottom: 1px solid #e6e6e6;
}

.add_pic {
    width: 102rpx;
    height: 102rpx;
    background-color: #f2f2f2;
    margin-top: 10rpx;
}

.pic_content {
    font-size: 24rpx;
    padding-bottom: 30rpx;
}

.general_style3 {
    display: flex;
    justify-content: center;
    align-items: center;
}

.shizi_style {
    transform: rotate(45deg);
}

.add_pic_tohouse {
    height: 82rpx;
    width: 100%;
    background-color: white;
    margin-top: 10rpx;
    font-size: 32rpx;
    font-weight: 700;
}

.add_submit {
    position: fixed;
    left: 0;
    bottom: 0;
    width: 100%;
    height: 90rpx;
    background-color: white;
    z-index: 7;
}

.add_submit_item {
    width: 690rpx;
    height: 78rpx;
    background-color: #3772cc;
    border-radius: 8rpx;
}

.add_submit_text {
    font-size: 30rpx;
    color: white;
}

.upload-file {
    clear: both;
    overflow: hidden;
}

.upload-file>view {
    float: left;
    margin-right: 5rpx;
}

.upload-file .pic {
    width: 104rpx;
    height: 104rpx;
    box-sizing: border-box;
    margin-top: 10;
    border-radius: 4px;
    position: relative;
    margin-bottom: 60rpx;
    background: #f2f2f2 url(data:image/png;base64,) center / 20px no-repeat;
}

.upload-file .small-level-color {
    display: inline-block;
    width: 90rpx;
    text-align: center;
    font-size: 20rpx;
    margin-top: 4px;
}