@import "../__wuBaseWxss__/0.wxss";

.page .background {
    position: relative;
    z-index: 1;
    width: 100%;
    overflow: hidden;
}

.page .qrcode {
    position: relative;
    z-index: 2;
    margin: -123rpx auto 0;
    width: 279rpx;
    border-radius: 6rpx;
    overflow: hidden;
    box-shadow: 1rpx 1rpx 13rpx 0 rgba(0,0,0,0.21);
}

.page .qrcode image {
    width: 100%;
    overflow: hidden;
}

.page .add-card {
    width: 712rpx;
    height: 295rpx;
    margin: 30rpx auto 0;
    overflow: hidden;
    background: url("data:image/png;base64,") 100% / cover no-repeat;
}

.page .add-card .text,.page .add-card .btn {
    text-align: center;
}

.page .add-card .text {
    margin-top: 80rpx;
    color: #000;
}

.page .add-card .btn {
    margin: 30rpx auto 0;
    width: 237rpx;
    height: 67rpx;
    line-height: 67rpx;
    border-radius: 34rpx;
    background-color: #2d8cf0;
    overflow: hidden;
    box-shadow: 0 6rpx 18rpx 0 rgba(45,140,240,0.24);
}