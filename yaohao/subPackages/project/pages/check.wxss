@import "../__wuBaseWxss__/0.wxss";

page {
    background-color: #f7f7f7;
}

.checkTitle {
    width: 100%;
    height: 90rpx;
    padding: 0 30rpx;
    background-color: #FFFFFF;
    box-sizing: border-box;
    display: flex;
    align-items: center;
    flex-direction: row;
    justify-content: space-between;
}

.checkTitle .titleLeft {
    line-height: 50rpx;
    font-size: 25rpx;
    font-weight: bold;
}

.checkTitle .titleRight {
    width: 55rpx;
    height: 50rpx;
}

.checkTitle .titleRight button {
    width: 100%;
    height: 100%;
    background-color: transparent;
    font-size: inherit;
}

.checkTitle .titleRight image {
    width: 100%;
    height: 100%;
}

.PictureMsg {
    width: 100%;
    margin-top: 20rpx;
    padding: 45rpx 30rpx 20rpx 30rpx;
    box-sizing: border-box;
    background-color: #FFFFFF;
}

.PictureMsg .txt {
    padding: 0 50rpx;
    font-size: 34rpx;
}

.PictureMsg .txtLine {
    flex: 1;
    display: flex;
    align-items: center;
}

.PictureMsg .txtLine image {
    width: 100%;
}

.PictureMsg .PictureTitle {
    display: flex;
    flex-direction: row;
    flex-wrap: nowrap;
    align-items: center;
}

.PictureMsg .sendPicture {
    margin-top: 54rpx;
}

.txtLine image {
    width: 100%;
}

.checkBtn {
    text-align: center;
    padding: 40rpx 0 180rpx 0;
}

.checkBtn .savePicture {
    display: inline-block;
    line-height: 78rpx;
    height: 78rpx;
    border-radius: 50rpx;
    border: 1px solid #3773cb;
    overflow: hidden;
    color: #3773cb;
    padding: 0 95rpx;
    font-size: 28rpx;
    text-align: center;
    margin: 0 auto;
    background-color: #fff;
}