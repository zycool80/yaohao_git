@import "../__wuBaseWxss__/1.wxss";

@import "../__wuBaseWxss__/0.wxss";

.search-bar {
    position: fixed;
    left: 0;
    top: 0;
    width: 100%;
    height: 50px;
    box-sizing: border-box;
    padding: 8px 20rpx 0;
    background-color: #3772cc;
    z-index: 999;
}

.search-bar .search-bar-form {
    background: #fff;
    line-height: 100%;
    color: #ccc;
    border-radius: 4px;
    position: relative;
    margin: 0 10rpx 20rpx;
    z-index: 1;
    overflow: hidden;
}

.search-bar .search-bar-form .weui-icon-clear,.search-bar .search-bar-form .weui-icon-search {
    width: 76rpx;
    height: 66rpx;
    line-height: 66rpx;
    text-align: center;
    font-size: 38rpx;
    color: #ccc;
    padding: 0;
}

.search-bar .search-bar-form .search-bar-input {
    width: auto!important;
    margin: 0 76rpx;
    height: 66rpx;
    color: #666;
}

.search-bar .search-bar-form .weui-icon-clear {
    position: relative;
    float: right;
    z-index: 2;
    left: 0;
}

.search-bar .search-bar-form .weui-icon-search {
    position: relative;
    float: left;
    z-index: 2;
    right: 0;
    margin-right: 0;
}

.search-bar .search-bar-icon {
    width: 70rpx;
    text-align: center;
    z-index: 2;
    line-height: 1;
    background-color: transparent;
    overflow: hidden;
}

.search-bar .float-left {
    float: left;
}

.search-bar .float-right {
    float: right;
}

.search-bar .search-bar-icon button.e_btn {
    width: 100%;
    height: 44rpx;
    line-height: 44rpx;
    padding: 0;
    background-color: transparent;
    font-size: 44rpx;
}

.search-bar .search-bar-icon text {
    line-height: 1;
}

.search-bar .search-bar-icon button::after {
    display: none;
}

.search-bar .search-input-place {
    color: #ccc;
}

.search-bar .search-btn {
    float: right;
    height: 70rpx;
    width: 80rpx;
    line-height: 70rpx;
    text-align: center;
    padding-left: 15rpx;
}

.search-bar-input {
    padding: 0;
    height: 66rpx;
}

.search-bar-input.platform {
    padding-top: 18rpx;
    box-sizing: border-box;
}

.banner {
    width: 100%;
    height: 300rpx;
    overflow: hidden;
}

.banner swiper-item {
    position: relative;
    z-index: 22;
}

.banner .advert {
    font-size: 18rpx;
    line-height: 26rpx;
    text-align: center;
    background: rgba(0,0,0,0.3);
    width: 50rpx;
    height: 26rpx;
    position: absolute;
    right: 20rpx;
    bottom: 15rpx;
    z-index: 222;
    border-radius: 1rpx;
    opacity: .4;
}

.weui-media-box__title {
    position: relative;
    line-height: 1;
    overflow: hidden;
}

.weui-media-box__title .left {
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    word-wrap: break-word;
    word-break: break-all;
    margin-right: 200rpx;
}

.weui-media-box__title .right {
    float: right;
}

.weui-media-box__title .right .height-level-color {
    font-weight: normal;
}

.weui-media-box__title .right .square {
    position: relative;
    top: -3px;
}

.item_content {
    display: flex;
    margin: 0 30rpx 30rpx;
    border-bottom: 1rpx solid #e2e2e2;
}

.item_content .area {
    text-overflow: ellipsis;
    overflow: hidden;
    white-space: wrap;
    display: block;
    margin-right: 220rpx;
    word-wrap: break-all;
    height: 36rpx;
}

.item_content .content_right .tags {
    margin: 15rpx 0;
    line-height: 1;
    overflow: hidden;
}

.item_content .content_right .list_bottom .attention {
    line-height: 1;
    float: right;
    width: 260rpx;
    text-align: right;
}

.item_content .content_right .list_bottom .follow {
    line-height: 1;
}

.hot_house_img {
    float: left;
    width: 210rpx;
    height: 140rpx;
    position: relative;
}

.hot_house_img .lottery-img {
    display: block;
    width: 210rpx;
    height: 140rpx;
    position: absolute;
    z-index: 1;
    top: 0;
}

.hot_house_img .view {
    position: absolute;
    bottom: 0;
    left: 10rpx;
    z-index: 3;
}

.hot_house_img .view>text {
    vertical-align: middle;
}

.hot_house_img .lottery-img-bottom {
    display: block;
    width: 100rpx;
    height: 30rpx;
    background-color: #f07062;
    line-height: 30rpx;
    text-align: center;
    position: absolute;
    z-index: 2;
    top: 0;
    left: 0;
}

.tag1 {
    height: 36rpx;
    line-height: 36rpx;
    display: inline-block;
    float: right;
}

.list_bottom {
    width: 100%;
    overflow: hidden;
    padding-bottom: 24rpx;
}

.attention .comment_follow {
    display: inline-block;
}

.sign-text {
    margin-right: 4px;
    height: 36rpx;
    line-height: 36rpx;
    display: inline-block;
}

.content_right {
    margin-left: 110rpx;
    width: 100%;
}

.title_name {
    width: 250rpx;
    overflow: hidden;
    white-space: normal;
    text-overflow: ellipsis;
}

.gray-shadow {
    position: absolute;
    width: 210rpx;
    height: 100%;
    z-index: 2;
    background: linear-gradient(to top,rgba(0,0,0,0.4) 0,transparent 30%);
}

.search-bar-input {
    padding-left: 0;
}

.projects {
    position: relative;
    z-index: 1;
    width: 100%;
    overflow: hidden;
}

.not_id_card {
    display: flex;
    align-items: center;
    justify-content: center;
    flex-direction: column;
    height: 740rpx;
    width: auto;
    background-color: #fff;
}

.project-nav {
    width: 100%;
    background-color: #fff;
    overflow: hidden;
}

.project-nav .project-menu-cont {
    width: 100%;
    box-sizing: border-box;
    padding: 0 30rpx;
    background-color: #fff;
    display: -webkit-flex;
    -webkit-flex-direction: row;
    -webkit-justify-content: space-between;
}

.project-nav .project-menu-cont .menu-select {
    display: -webkit-flex;
    -webkit-flex-direction: row;
    -webkit-justify-content: center;
    -webkit-align-items: center;
    height: 80rpx;
    overflow: hidden;
}

.project-nav .project-menu-cont .menu-select>text {
    line-height: 1;
}

.project-nav .project-menu-cont .menu-select>text.iconfont {
    margin-left: 10rpx;
    font-size: 16rpx;
}

.project-nav .project-menu-cont .menu-select>text.icon-paixu {
    margin: 0 20rpx;
}

.project-nav .project-menu-cont .menu-select.active>text {
    color: #2d8cf0!important;
}

.project-nav.fixed-top {
    position: fixed;
    width: 100%;
    height: 100%;
    top: 100rpx;
    left: 0;
    z-index: 2;
    background-color: rgba(0,0,0,0.5);
}

.project-nav.fixed-top .close-select {
    position: absolute;
    z-index: 1;
    width: 100%;
    height: 100%;
    background: transparent;
}

.project-nav.fixed-top .project-nav-cont,.project-nav.fixed-top .project-menu-cont {
    position: relative;
    z-index: 2;
}

.project-select-content {
    width: 100%;
    background-color: #fff;
    overflow: hidden;
}

.project-select-content .select-left-cont {
    position: relative;
    float: left;
    width: 180rpx;
    height: 630rpx;
    overflow: scroll;
    z-index: 2;
}

.project-select-content .select-left-cont .select-item {
    width: 100%;
    line-height: 90rpx;
    height: 90rpx;
    color: #1a1a1a;
    text-indent: 30rpx;
    overflow: hidden;
}

.project-select-content .select-left-cont .select-item.active {
    color: #2d8cf0;
}

.project-select-content .select-right-cont {
    position: relative;
    margin-left: 180rpx;
    height: 630rpx;
    overflow: scroll;
    z-index: 1;
}

.project-select-content .select-right-cont .select-item_list {
    text-indent: 30rpx;
    line-height: 90rpx;
    height: 90rpx;
    border-bottom: 1rpx solid #e6e6e6;
    box-sizing: border-box;
    color: #1a1a1a;
}

.project-select-content .select-right-cont .select-item_title {
    width: 100%;
    height: 80rpx;
    font-size: 26rpx;
    text-indent: 30rpx;
    line-height: 80rpx;
    overflow: hidden;
}

.project-select-content .select-right-cont .select-item_group {
    width: 100%;
    box-sizing: border-box;
    padding-left: 30rpx;
    overflow: hidden;
}

.project-select-content .select-right-cont .select-item_group .select-item_btn {
    float: left;
    width: 148rpx;
    height: 58rpx;
    line-height: 58rpx;
    margin: 0 20rpx 20rpx 0;
    color: #1a1a1a;
    border-radius: 2rpx;
    background-color: #fff;
    text-align: center;
    border: 1rpx solid #fff;
}

.project-select-content .select-right-cont .select-item_group .select-item_btn.active {
    border: 1rpx solid #9acbff;
}

.project-select-content .select-right-cont .active {
    color: #2d8cf0!important;
}

.project-select-content .select-right-cont.no-left-cont {
    margin-left: 0;
}

.project-select-content .select-right-cont.tow-columns .short-column {
    width: 160rpx;
    height: 100%;
    float: left;
    overflow: scroll;
}

.project-select-content .select-right-cont.tow-columns .long-column {
    display: block;
    height: 100%;
    margin-left: 200rpx;
    overflow: scroll;
}

.project-select-content .select-bottom {
    width: 100%;
    height: 100rpx;
    background-color: #fff;
    box-sizing: border-box;
    padding: 10rpx 30rpx;
}

.project-select-content .select-bottom .select-input {
    float: left;
    width: 335rpx;
    height: 100%;
    border-radius: 10rpx;
    padding: 0 22rpx;
    box-sizing: border-box;
    display: -webkit-flex;
    -webkit-justify-content: space-between;
    -webkit-align-items: center;
}

.project-select-content .select-bottom .select-input input {
    width: 120rpx;
    height: 100%;
    line-height: 100%;
    font-size: 28rpx;
    overflow: hidden;
}

.project-select-content .select-bottom .select-btn {
    width: 335rpx;
    line-height: 80rpx;
    height: 80rpx;
    border-radius: 10rpx;
    text-align: center;
    background: linear-gradient(to right,#2d8cf3,#1067eb);
    color: #fff;
}

.project-select-content .select-bottom .select-btn.white-style {
    background: #fff;
    box-sizing: border-box;
    border: 1px solid #6f99c4;
    color: #6f99c4;
}

.project-select-content .gray-f7-bg {
    background-color: #f7f7f7;
}

.project-select-content .fz-14 {
    font-size: 28rpx!important;
}

.project-select-content .float_left {
    float: left;
}

.project-select-content .float_right {
    float: right;
}

.project-select_sort {
    height: 520rpx;
}

.project-select_sort .select-right-cont {
    height: 420rpx;
}