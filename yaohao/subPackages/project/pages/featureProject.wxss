}

.rotate {
    transform: rotate(180deg);
    display: inline-block;
}

.project-nav {
    width: 100%;
    background-color: #fff;
    overflow: hidden;
}

.project-nav .project-nav-cont {
    width: 100%;
    border-bottom: 1rpx solid #e6e6e6;
    overflow: hidden;
    background-color: #fff;
}

.project-nav .project-nav-cont .item {
    float: left;
    width: 25%;
    height: 100rpx;
    line-height: 100rpx;
    overflow: hidden;
    box-sizing: border-box;
    text-align: center;
    position: relative;
}

.project-nav .project-nav-cont .item text {
    color: #999;
    font-weight: bold;
}

.project-nav .project-nav-cont .item.active text {
    color: #db5643;
}

.project-nav .project-nav-cont .item.active text:after {
    position: absolute;
    display: block;
    content: '';
    width: 50%;
    bottom: 0;
    left: 50%;
    -webkit-transform: translateX(-50%);
    height: 6rpx;
    background-color: #db5643;
}

.project-nav .project-menu-cont {
    width: 100%;
    box-sizing: border-box;
    padding: 0 30rpx;
    background-color: #fff;
    display: -webkit-flex;
    -webkit-flex-direction: row;
    border-top: 1rpx solid #e6e6e6;
    border-bottom: 1rpx solid #e6e6e6;
    -webkit-justify-content: space-between;
}

.project-nav .project-menu-cont .menu-select {
    display: -webkit-flex;
    -webkit-flex-direction: row;
    -webkit-justify-content: center;
    -webkit-align-items: center;
    height: 80rpx;
    overflow: hidden;
}

.project-nav .project-menu-cont .menu-select>text {
    line-height: 1;
}

.project-nav .project-menu-cont .menu-select>text.iconfont {
    margin-left: 10rpx;
    font-size: 16rpx;
}

.project-nav .project-menu-cont .menu-select>text.icon-paixu {
    margin: 0 20rpx;
}

.project-nav .project-menu-cont .menu-select.active>text {
    color: #db5643!important;
}

.project-nav.fixed-top {
    position: fixed;
    width: 100%;
    height: 100%;
    top: 0;
    left: 0;
    z-index: 22;
    background-color: rgba(0,0,0,0.5);
}

.project-nav.fixed-top .close-select {
    position: absolute;
    z-index: 1;
    width: 100%;
    height: 100%;
    background: transparent;
}

.project-nav.fixed-top .project-nav-cont,.project-nav.fixed-top .project-menu-cont {
    position: relative;
    z-index: 2;
}

.project-select-content {
    width: 100%;
    background-color: #fff;
    overflow: hidden;
}

.project-select-content .select-left-cont {
    position: relative;
    float: left;
    width: 180rpx;
    height: 630rpx;
    overflow: scroll;
    z-index: 2;
}

.project-select-content .select-left-cont .select-item {
    width: 100%;
    line-height: 90rpx;
    height: 90rpx;
    color: #1a1a1a;
    text-indent: 30rpx;
    overflow: hidden;
}

.project-select-content .select-left-cont .select-item.active {
    color: #db5643;
}

.project-select-content .select-right-cont {
    position: relative;
    margin-left: 180rpx;
    height: 630rpx;
    overflow: scroll;
    z-index: 1;
}

.project-select-content .select-right-cont .select-item_list {
    text-indent: 30rpx;
    line-height: 90rpx;
    height: 90rpx;
    border-bottom: 1rpx solid #e6e6e6;
    box-sizing: border-box;
    color: #1a1a1a;
}

.project-select-content .select-right-cont .select-item_title {
    width: 100%;
    height: 80rpx;
    font-size: 26rpx;
    text-indent: 30rpx;
    line-height: 80rpx;
    overflow: hidden;
}

.project-select-content .select-right-cont .select-item_group {
    width: 100%;
    box-sizing: border-box;
    padding-left: 30rpx;
    overflow: hidden;
}

.project-select-content .select-right-cont .select-item_group .select-item_btn {
    float: left;
    width: 148rpx;
    height: 58rpx;
    line-height: 58rpx;
    margin: 0 20rpx 20rpx 0;
    color: #1a1a1a;
    border-radius: 2rpx;
    background-color: #fff;
    text-align: center;
    border: 1rpx solid #fff;
}

.project-select-content .select-right-cont .select-item_group .select-item_btn.active {
    border: 1rpx solid #db5643;
}

.project-select-content .select-right-cont .active {
    color: #db5643!important;
}

.project-select-content .select-right-cont.no-left-cont {
    margin-left: 0;
}

.project-select-content .select-right-cont.tow-columns .short-column {
    width: 160rpx;
    height: 100%;
    float: left;
    overflow: scroll;
}

.project-select-content .select-right-cont.tow-columns .long-column {
    display: block;
    height: 100%;
    margin-left: 200rpx;
    overflow: scroll;
}

.project-select-content .select-bottom {
    width: 100%;
    height: 100rpx;
    background-color: #fff;
    box-sizing: border-box;
    padding: 10rpx 30rpx;
}

.project-select-content .select-bottom .select-input {
    float: left;
    width: 335rpx;
