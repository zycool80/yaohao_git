}

.project-select-content .select-bottom .select-btn {
    width: 335rpx;
    line-height: 80rpx;
    height: 80rpx;
    border-radius: 10rpx;
    text-align: center;
    background: #de5646;
    color: #fff;
}

.project-select-content .select-bottom .select-btn.white-style {
    background: #fff;
