@import "../__wuBaseWxss__/0.wxss";

page {
    background: #f7f7f7;
}

.left-title {
    float: left;
    width: 150rpx;
    height: 80rpx;
    line-height: 80rpx;
}

.right-title {
    margin-left: 150rpx;
    position: relative;
    height: 80rpx;
    line-height: 80rpx;
}

.chooseType-btn {
    background: #e6fdff;
    padding: 10rpx 20rpx;
    line-height: 1;
    border-radius: 32rpx;
}

.chooseType-text {
    position: relative;
    float: left;
    margin-left: 20rpx;
}

.delete-btn {
    width: 22rpx;
    height: 22rpx;
    border-radius: 22rpx;
    background: #ccc;
    text-align: center;
    line-height: 22rpx;
    position: absolute;
    right: -5rpx;
    top: 10rpx;
    font-size: 12rpx;
    font-weight: bold;
}

.top {
    padding-bottom: 30rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
}

.chooseType {
    min-height: 40rpx;
}

.upForm {
    margin: 70rpx 30rpx 10rpx;
    line-height: 80rpx;
    height: 80rpx;
    text-align: center;
    box-sizing: border-box;
    color: #fff;
    background-color: #3dc4d0;
    border-radius: 8rpx;
}

.askQuestion-top {
    padding-left: 30rpx;
    border-bottom: 1rpx solid #f7f7f7;
}

.askQuestion-top,.askQuestion-middle {
    background: #fff;
}

.textArea-box-text {
    display: block;
    width: 16px;
    position: absolute;
    bottom: 15rpx;
    right: 60rpx;
}

.left-title-middle {
    width: 150rpx;
    padding: 30rpx;
}

.right-title-middle {
    padding: 40rpx 0 30rpx;
    margin-left: 210rpx;
    position: relative;
}

.right-title-middle taxtarea {
    padding-right: 20rpx;
}

.top-right {
    float: right;
    padding-right: 30rpx;
}

.choose-line-title {
    padding-bottom: 25rpx;
}

.choose-line-title:before {
    position: relative;
    top: 5rpx;
    display: inline-block;
    width: 6rpx;
    height: 26rpx;
    content: '';
    background: #3dc4d0;
}

.choose-line-title>text {
    padding-left: 10rpx;
}

.choose-content {
    padding: 40rpx 30rpx;
}

.choose-btn {
    padding-bottom: 30rpx;
}

.choose-line {
    padding: 30rpx 0 0;
}

.bottom-tip {
    padding: 30rpx 30rpx 0;
}

.choose-btn {
    display: flex;
    justify-content: space-between;
}

.bottom-btn {
    display: flex;
    justify-content: space-between;
    text-align: left;
}

.bottom-btn>view {
    width: 50%;
}

.model-bg {
    width: 100%;
    height: 100%;
    position: fixed;
    left: 0;
    background: rgba(0,0,0,0.1);
    top: 0;
    z-index: 999;
}

.model-box {
    position: fixed;
    bottom: 0;
    width: 100%;
    z-index: 1000;
    background: #fff;
}

.choose-content .choose-line .choose-checkbox-cont {
    display: flex;
    justify-content: flex-start;
    flex-direction: row;
}

.choose-content .choose-line .choose-checkbox-cont .choose-checkbox {
    width: 25%;
    line-height: 30rpx;
}

.choose-content .choose-line .choose-checkbox-cont .choose-checkbox.bottom-checkbox {
    width: 50%!important;
}

.choose-content .choose-line .choose-checkbox-cont .choose-checkbox>.checkbox {
    display: inline-block;
    float: left;
    width: 26rpx;
    height: 26rpx;
    background: #fff;
    border: 1px solid #999;
    margin-right: 20rpx;
    overflow: hidden;
}

.choose-content .choose-line .choose-checkbox-cont .choose-checkbox>.checkbox.disable {
    background: #ccc;
}

.choose-content .choose-line .choose-checkbox-cont .choose-checkbox>.checkbox.checked {
    background: #fff url("data:image/png;base64,") center no-repeat;
    background-size: 24rpx;
}

.place-hold {
    margin-top: 12rpx;
    color: #999;
}

.lineHeight1 {
    line-height: 1;
}

.upload-file {
    clear: both;
    margin-top: 20rpx;
    overflow: hidden;
    padding: 0 20rpx;
}

.upload-file>view {
    float: left;
    margin-right: 5rpx;
}

.upload-file .pic {
    width: 100rpx;
    height: 100rpx;
    box-sizing: border-box;
    border-radius: 4px;
    position: relative;
    margin-bottom: 60rpx;
    background: #f2f2f2 url(data:image/png;base64,) center / 25px no-repeat;
}

.upload-file .black6-color {
    display: inline-block;
    width: 90rpx;
    text-align: center;
    font-size: 20rpx;
    margin-top: 4px;
}

.doudou {
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    background-color: white;
    margin-top: 10rpx;
}

.upFor {
    margin: 70rpx 30rpx 10rpx;
    line-height: 80rpx;
    height: 80rpx;
    text-align: center;
    box-sizing: border-box;
    color: #fff;
    background-color: #cccccc;
    border-radius: 8rpx;
}