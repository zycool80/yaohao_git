@import "../__wuBaseWxss__/0.wxss";

.page,page {
    height: 100%;
}

.salesManTip {
    background: #f5f5f5;
    height: 100%;
}

.saleMansHead {
    text-align: center;
    position: relative;
    z-index: 25;
    height: 300rpx;
}

.saleMansHead image {
    width: 100%;
    position: absolute;
    left: 0;
    top: 0;
    z-index: 22;
}

.saleMansHead .true-image {
    color: #fff;
    font-size: 100rpx;
    display: inline-block;
}

.saleMansHead .z23 {
    position: relative;
    z-index: 25;
}

.saleMansContent {
    margin: 0 30rpx;
}

.saleMansContent view:first-child {
    padding: 30rpx 0;
    color: #6d6d6d;
}

.salesManInfo {
    text-align: center;
}

.salesManInfo image {
    display: inline-block;
}

.salesManBtn {
    text-align: center;
    padding: 30rpx;
}

.salesManBtn button {
    background: #fff;
    border-radius: 80rpx;
    height: 80rpx;
    width: 400rpx;
    line-height: 80rpx;
    display: block;
    margin: 15rpx auto;
}

.salesManBtn navigator {
    display: block;
    height: 80rpx;
    width: 400rpx;
    line-height: 80rpx;
    border-radius: 80rpx;
    background: #fff;
    margin: 15rpx auto;
}